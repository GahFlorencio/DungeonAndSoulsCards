<svg width="120" height="168" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4a3728;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#6b4d37;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#3d2a1f;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="borderGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#b8860b;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#b8860b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Fundo da carta -->
  <rect width="120" height="168" fill="url(#cardGradient)" rx="8" ry="8"/>
  
  <!-- Borda decorativa -->
  <rect x="2" y="2" width="116" height="164" fill="none" stroke="url(#borderGradient)" stroke-width="4" rx="6" ry="6"/>
  
  <!-- Padrão decorativo central -->
  <g transform="translate(60,84)">
    <!-- Símbolo central -->
    <circle cx="0" cy="0" r="25" fill="#8b6914" opacity="0.8"/>
    <text x="0" y="-15" font-family="serif" font-size="16" text-anchor="middle" fill="#ffd700">⚜️</text>
    <text x="0" y="0" font-family="serif" font-size="20" text-anchor="middle" fill="#ffd700">🗡️</text>
    <text x="0" y="15" font-family="serif" font-size="16" text-anchor="middle" fill="#ffd700">⚜️</text>
  </g>
  
  <!-- Decorações nos cantos -->
  <text x="15" y="25" font-family="serif" font-size="12" fill="#8b6914">⚜️</text>
  <text x="105" y="25" font-family="serif" font-size="12" fill="#8b6914">⚜️</text>
  <text x="15" y="155" font-family="serif" font-size="12" fill="#8b6914">⚜️</text>
  <text x="105" y="155" font-family="serif" font-size="12" fill="#8b6914">⚜️</text>
</svg>
